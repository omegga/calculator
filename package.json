{
  "name": "calculator",
  "version": "1.0.0",
  "description": "Calculator",
  "main": "index.js",
  "dependencies": {
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.2",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "postcss-loader": "^3.0.0",
    "prettier": "1.17.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.1",
    "stylelint-config-prettier": "^5.1.0",
    "stylelint-prettier": "^1.0.7",
    "stylelint-webpack-plugin": "^0.10.5",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "scripts": {
    "dev": "webpack-dev-server --config webpack.dev.js",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "fix-code": "eslint --fix front/**/*.{js,jsx}",
    "fix-style": "stylelint --fix front/**/*.css",
    "prod": "webpack --config webpack.prod.js",
    "serve": "node server",
    "start": "npm run build:prod && npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omegga/calculator.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/omegga/calculator/issues"
  },
  "homepage": "https://github.com/omegga/calculator#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "front/**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "front/**/*.css": [
      "stylelint --fix",
      "git add"
    ],
    "server/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
